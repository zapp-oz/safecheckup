<html>
    <head>
        <title>Chat</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
        <link href="https://fonts.googleapis.com/css2?family=Abel&family=Bebas+Neue&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="/stylesheets/chat.css">
    </head>
    <body style="background-color: #eeeeee;">
        <div id="chat-header">
            <h2>MESSAGE</h2>
        </div>
        <div id="chat-box">
            <div id="chat-messages">
                
            </div>
            <div id="message-box">
                <input type="text" id="input-box" autocomplete="off" placeholder="message">
                <button disabled id="send-btn">Send</button>
            </div>
        </div>
        <script src="/socket.io/socket.io.js"></script>
        <script>
            let cred = {
                user: '<%- JSON.stringify(username)%>',
                room: '<%- JSON.stringify(room)%>'
            }
        </script>
        <script src="/scripts/chat.js"></script>
    </body>
</html>
